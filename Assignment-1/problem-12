#include <stdio.h>
int main() {
int N, i;
printf("Enter the value of N: ");
scanf("%d", &N);
int firstnumber = 1, secondnumber = 1;
printf("Fibonacci series : %d, %d", N, firstnumber, secondnumber);
for (i = 3; i <= N; i++) {
int nextnumber = firstnumber + secondnumber;
printf(", %d", nextnumber);
firstnumber = secondnumber;
secondnumber = nextnumber;
}

return 0;
}
